n=int(input("Enter the number of elemnts : "))
list1=[]
for i in range (n):
    a=input("Enter the number to add on list :")
    list1.append(a)
m=int(input("Enter the number of elements :"))
list2=[]
list3=[]
for i in range (m):
    b=input("Enter the number to add on list :")
    list2.append(b)
print(list1)
print(list2)
list3=(list1+list2)
list3.sort()
print("the merge of two list is",list3)
